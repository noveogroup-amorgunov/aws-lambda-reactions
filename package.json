{
  "name": "aws-lambda-reactions",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "local": "IS_OFFLINE=true sls offline start",
    "logs:set-reaction": "sls logs --function setReaction -t",
    "logs:get-reactions": "sls logs --function getReactions -t",
    "deploy": "sls deploy",
    "lint": "eslint src/**/*.ts",
    "test": "NODE_ENV=test NODE_PATH=. jest"
  },
  "keywords": [],
  "author": "Alexander Morgunov <fxl@list.ru>",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/jest": "25.2.1",
    "eslint": "5.14.0",
    "eslint-config-iddqd": "1.2.0",
    "eslint-plugin-import": "2.16.0",
    "husky": "1.3.1",
    "jest": "25.4.0",
    "nock": "10.0.6",
    "serverless": "1.68.0",
    "serverless-dotenv-plugin": "2.0.1",
    "serverless-dynamodb-local": "0.2.39",
    "serverless-offline": "4.5.0",
    "serverless-plugin-typescript": "1.1.9",
    "ts-jest": "^25.4.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "aws-sdk": "2.814.0"
  },
  "eslintConfig": {
    "extends": "iddqd/node",
    "parserOptions": {
      "ecmaVersion": 2019
    },
    "rules": {
      "camelcase": 0,
      "capitalized-comments": 0
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "jest": {
    "transform": {
      ".+\\.tsx?$": "ts-jest"
    }
